{
    "defaultSeverity": "error",
    "extends": ["tslint:recommended", "tslint-plugin-prettier", "tslint-config-prettier"],
    "linterOptions": {
        "exclude": "test/**/*.ts"
    },
    "rules": {
        "interface-name": false,
        "variable-name": {
            "options": [
                "ban-keywords",
                "check-format",
                "allow-leading-underscore"
            ]
        },
        "prettier": true
    }
}
